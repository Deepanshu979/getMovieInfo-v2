<%- include("../layouts/header") %> <%- include('../layouts/navbar') %>

<div id="container">
  <div id="cont">
    <% if(!shows){ %>
    <h1>No Such Shows/Movies</h1>
    <% } else {%> <% for(var show of shows){ %> <% if(show.Poster){ %>
    <div class="searchedMovie">
      <img
        onclick="movieDetails(id)"
        src="<%= show.Poster %>"
        alt=""
        id="<%=show.imdbID %>"
        style="height: 300px; width: 300px"
      />
      <h4 class="name mt-3"><%= show.Title %></h4>
    </div>
    <% } %> <% } %> <% } %>
  </div>
</div>

<%- include("../layouts/footer") %>
