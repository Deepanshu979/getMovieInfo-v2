<%- include("../layouts/header") %> <%- include('../layouts/navbar') %>

<div id="container">
  <div id="cont">
    <% if(!shows.length){ %>
    <h1>No Such Shows/Movies</h1>
    <% } else {%> <% for(var show of shows){ %> <% if(show.show.image){ %>
    <div class="searchedMovie">
      <img
        onclick="searchedMovie(id)"
        src="<%= show.show.image.original %>"
        alt=""
        id="<%=show.show.name %>"
        style="height: 300px; width: 300px"
      />
      <h4 class="name mt-3"><%= show.show.name %></h4>
    </div>
    <% } %> <% } %> <% } %>
  </div>
</div>

<%- include("../layouts/footer") %>
